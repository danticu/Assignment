  Question 5-
  class robert{
public static void main(String[] args) {
    float p=0;
int sub1= 78;
int sub2=45;
int sub3=62;
int sum= sub1+sub2+sub3;
p=(sum * 100)/ 300;
System.out.println("total of marks are:" +sum);
System.out.println("percentage of marks are:" +p);
}}

Question 6-
public class Convert {

	public static void main(String[] args) {
		float Fahrenheit, Celsius;
		Fahrenheit = 43;
		Celsius = ((Fahrenheit-32)*5)/9;
		System.out.println("Temperature in celsius is: "+Celsius);
		}
	}
  
  Question 7-
  public class Newnumber {

	public static void main(String[] args) {
		int n=2345, one, two, three, four,last;
		 one = (n / 1000);
		 n = (n % 1000) + 2;
		 two = (n / 100) + 2;
		 n = (n % 100) + 2;
		 three = (n / 10) + 2;
		 n = (n % 10) + 2; 
		 four = (n / 1) + 2;
		 n = (n % 1) + 2; 
		last = (one * 1000) + (two * 100) + (three * 10) + four;
		System.out.println("final" + last);
	}

}

Question 8-
import java.util.Scanner;

public class Rectangle {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enetr the length");
		int x=sc.nextInt();
		System.out.println("enetr the breadth");
		int y=sc.nextInt();
int perimeter=x+y;
System.out.println("perimeter is" + perimeter);
int area=x*y;
System.out.println("area is"  + area);	

}

Question 9-
public class Reverse {

	public static void main(String[] args) {
		 int num = 123, reversed = 0;

		    for(;num != 0; num /= 10) {
		      int digit = num % 10;
		      reversed = reversed * 10 + digit;
		    }

		    System.out.println("Reversed Number: " + reversed);
	}

}
 
